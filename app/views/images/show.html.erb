<%= render partial: 'images/flash' %>

<% if @image.present? %>
  <%= image_tag(@image.url) %>
  <%= link_to "Delete", image_path(@image), class: 'js-delete_btn', method: :delete, data: {confirm: 'Are you sure?'} %>
  <br>
  <ul>
    <%= render partial: 'images/tag_list', locals: {image: @image} %>
  </ul>
<% end %>

<%= button_to "Index Page", images_path, method: :get, class: 'js-index_page'%>
