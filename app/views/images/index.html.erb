<h1> Images </h1>
<% stylesheet_link_tag "images.css" %>
<ul>
  <% @images.each do |image| %>
      <li>
        <%= image_tag(image.url, class: 'index-page_image') %>
        <% if image.tag_list.present? %>
            <h4>Tag List</h4>
            <ul>
              <% image.tag_list.each do |tag| %>
                  <li><%= tag %></li>
              <% end %>
            </ul>
        <% end %>
      </li>
      <br><br>
  <% end %>
</ul>
